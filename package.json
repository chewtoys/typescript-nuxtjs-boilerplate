{
  "name": "typescript-nuxtjs",
  "version": "0.0.1",
  "description": "My gnarly Nuxt.js project",
  "author": "hisasann",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hisasann/typescript-nuxtjs.git"
  },
  "keywords": [
    "nuxt-ts",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/hisasann/typescript-nuxtjs/issues"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-beta.8",
    "@types/webpack": "^4.4.25",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt-ts": "^2.4.5",
    "vue-property-decorator": "^7.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/babel-core": "6.25.6",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.9",
    "@types/prettier": "1.16.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "1.16.4",
    "vue-jest": "^3.0.2"
  }
}
