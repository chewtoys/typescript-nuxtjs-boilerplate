{
  "name": "typescript-nuxtjs-boilerplate",
  "private": true,
  "version": "0.0.1",
  "description": "My gnarly Nuxt.js project",
  "scripts": {
    "local": "npm run dev",
    "dev": "cross-env NODE_ENV=development BUILD_ENV=local nuxt",
    "build": "nuxt build --no-lock",
    "build:static": "nuxt build --spa --no-lock",
    "build:local": "cross-env NODE_ENV=production BUILD_ENV=local nuxt build --no-lock",
    "build:docker": "cross-env NODE_ENV=production nuxt build --no-lock",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "clean:cache": "rimraf node_modules/.cache/hard-source",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "analyze": "cross-env NODE_ENV=production BUILD_ENV=local nuxt build --analyze",
    "docker": "docker-compose -f docker-compose.yml up --build",
    "docker:clean": "docker-compose -f docker-compose.yml build --no-cache && docker-compose -f docker-compose.yml up",
    "container-prune": "docker container prune",
    "image-prune": "docker image prune",
    "system-prune": "docker system prune",
    "stats": "docker container stats"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "author": {
    "name": "hisasann",
    "email": "hisasann25@gmail.com",
    "url": "https://github.com/hisasann"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hisasann/typescript-nuxtjs-boilerplate.git"
  },
  "keywords": [
    "nuxt",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/hisasann/typescript-nuxtjs-boilerplate/issues"
  },
  "dependencies": {
    "@nuxt/typescript": "^2.6.2",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "@nuxtjs/sentry": "^2.3.2",
    "@types/webpack": "^4.4.27",
    "@ungap/url-search-params": "^0.1.2",
    "animejs": "^3.0.1",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "nuxt": "^2.6.2",
    "nuxt-client-init-module": "^0.1.4",
    "nuxt-env": "^0.1.0",
    "nuxt-property-decorator": "^2.1.3",
    "ts-node": "^8.1.0",
    "vue-carousel": "^0.18.0",
    "vue-i18n": "^8.10.0",
    "vue-lazyload": "^1.2.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "@storybook/vue": "^5.0.9",
    "@types/animejs": "^2.0.2",
    "@types/babel-core": "6.25.6",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/js-cookie": "^2.2.2",
    "@types/moment": "^2.13.0",
    "@types/prettier": "1.16.2",
    "@types/storybook__vue": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.17.2",
    "eslint-plugin-jest": ">=22.4.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": ">=4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "prettier": "1.17.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "vue-jest": "^3.0.4"
  }
}
